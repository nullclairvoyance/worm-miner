# ============================================
# WORM Multi-Wallet Farming Script
# ============================================
# Copy to .env:  cp .env.example .env
# Protect keys:  chmod 600 .env

# ============================================
# NETWORK
# ============================================
RPC_URL=https://eth-sepolia.g.alchemy.com/v2/YOUR_API_KEY
NETWORK=sepolia

# ============================================
# WALLET PRIVATE KEYS (up to 5)
# ============================================
PK1=your_private_key_here
PK2=
PK3=
PK4=
PK5=

# ============================================
# BUDGET & MINING STRATEGY
# ============================================
# Total ETH to burn (converts to BETH)
TOTAL_ETH_BUDGET=0.05

# BETH to commit per epoch
# Example: 0.05 ETH → 0.04999 BETH → 49 epochs @ 0.001 each
BETH_PER_EPOCH=0.001

# Claim WORM rewards every N epochs
CLAIM_INTERVAL=5

# Protocol burn fee
BURN_FEE=0.00001

# ============================================
# PROVER ENDPOINTS (auto-failover)
# ============================================
# Primary prover URL
PROVER_URL=https://worm-miner-3.darkube.app

# Backup prover URL (auto-switches if primary fails)
PROVER_BACKUP_URL=https://worm-testnet.metatarz.xyz

# Timeout for proof generation (seconds)
PROVER_TIMEOUT=600

# ============================================
# ORCHESTRATION
# ============================================
LOOP_INTERVAL_SECONDS=600
MAX_RETRIES=3

# ============================================
# LOGGING
# ============================================
LOG_LEVEL=INFO
LOG_TO_FILE=true
LOG_FILE=logs/worm-farmer.log
